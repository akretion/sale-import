<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id ="exc_inconsistent_tax_amts" model="exception.rule">
        <field name="name">Inconsistent tax amounts</field>
        <field name="description">Real and calculated tax amounts are different</field>
        <field name="sequence">50</field>
        <field name="model">sale.order</field>
        <field name="code">failed = not obj.check_amounts_consistency()</field>
        <field name="active" eval="True"/>
    </record>
</odoo>
